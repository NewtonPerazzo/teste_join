Questão 1:

 create table cargos(
    nome varchar(200),
    primary key (nome)
);

create table pessoas(
	id int,
    nome varchar(200),
    cargo varchar(200),
    admissao date,
    foreign key(cargo) references cargos(nome)
);

 LETRA A:

 select pessoas.nome, pessoas.cargo from pessoas join cargos on cargos.nome = pessoas.cargo order by pessoas.admissao;

 LETRA B:

 select pessoas.nome, min(pessoas.admissao) from pessoas;

 LETRA C:

 select cargo, count(*) from pessoas group by cargo;

Questão 2:

 [ 5 - Function ] var e = function(){};
